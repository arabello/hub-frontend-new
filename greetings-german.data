gger: \",sgt\"\n    replace: \"Sehr geehrtes {{clipboard}}-Team,\\n\\n\\n\"\n\n  - trigger: \",gm\"\n    replace: \"Guten Morgen,\\n\\n\\n\"\n\n  - trigger: \"!mm\"\n    replace: \"Moin, Moin!\\n\\n\\n\"\n\n  - trigger: \"!dgg\"\n    replace: \"\\n\\nDanke sehr, viele Grüße & alles Gute!\"\n\n  - trigger: \"!gg\"\n    replace: \"\\n\\nViele Grüße & alles Gute!\"\n\n  - trigger: \"!ds\"\n    replace: \"\\n\\nDanke sehr!\"\n","repositoryHome","versions",[16],"isLatest",true]
